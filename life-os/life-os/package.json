{
	"name": "@life-os/life-os",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"exports": {
		"./types": {
			"import": "./dist/types/index.js",
			"types": "./dist/types/index.d.ts"
		},
		"./expression": {
			"import": "./dist/expression/index.js",
			"types": "./dist/expression/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"scripts": {
		"build": "npm run build:ts && npm run build:expression && node -e \"require('fs').accessSync('dist/expression/index.js')\"",
		"build:ts": "tsc -p tsconfig.build.json",
		"build:expression": "node rna/scripts/build-expression.mjs",
		"clean": "rimraf dist || rm -rf dist"
	},
	"devDependencies": {
		"fs-extra": "^11.3.2",
		"globby": "^15.0.0",
		"jsonc-parser": "^3.3.1",
		"rimraf": "^6.0.1",
		"slugify": "^1.6.6",
		"typescript": "^5.9.3",
		"zod": "^3.25.6"
	}
}
